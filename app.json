{
  "expo": {
    "name": "benTourism",
    "slug": "benTourism",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyBuNuN331jGDJRfk-BpFGt0EM59Joa4oo4"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBuNuN331jGDJRfk-BpFGt0EM59Joa4oo4"
        },
        "infoPlist": {
          "NSLocationAlwaysUsageDescription": "Allow $(PRODUCT_NAME) to use your location.",
          "NSLocationWhenInUseUsageDescription": "Allow $(PRODUCT_NAME) to use your location."
        }
      },
      "web": {
        "bundler": "metro",
        "output": "static",
        "favicon": "./assets/images/favicon.png"
      },
      "plugins": [
        "expo-router"
      ],
      "experiments": {
        "typedRoutes": true
      }
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ]
  }
}
